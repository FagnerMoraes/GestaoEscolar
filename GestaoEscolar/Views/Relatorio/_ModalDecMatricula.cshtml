<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<script src="~/Scripts/jquery-ui-1.11.4.js"></script>

<script type="text/javascript">
    $(function () {
        $("#termoBusca").autocomplete({
            source: '@Url.Action("GetAlunos")'
        });
    });
</script>

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">Declaração de Matricula</h3>
</div>



@using (Html.BeginForm("DeclaracaoMatricula", "Relatorio", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="modal-body">
        <div class="row">
            <div class="col-lg-offset-1 col-lg-10">
                <div class="input-group">
                    <input type="text" class="form-control" name="termoBusca" id="termoBusca" placeholder="Buscar por...">

                    <span class="input-group-btn">
                        <button type="submit" class="btn btn-default"><span class="fa fa-search"></span></button>
                    </span>
                </div>
            </div>
        </div>
    </div>


}

